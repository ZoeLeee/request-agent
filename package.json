{
  "name": "request-agent",
  "displayName": "Request agent",
  "version": "0.0.1",
  "description": "Intercepts and modifies specific network requests",
  "author": "Joe Lee",
  "scripts": {
    "clean": "rimraf build",
    "dev": "npm run clean && plasmo dev",
    "build": "npm run clean && plasmo build",
    "package": "npm run clean && plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.7.1",
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "prettier": "3.2.4",
    "rimraf": "^6.0.1",
    "tailwindcss": "3",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "webRequest",
      "<all_urls>",
      "storage",
      "tabs",
      "activeTab",
      "debugger"
    ],
    "background": {
      "service_worker": "background.ts"
    },
    "action": {
      "default_title": "Request Agent"
    }
  }
}
